<%- include('partials/header') %>
<div class="login-box">
  <h1>Sign Up</h1>
  <form action="/register" method="post">
  <div class="textbox">
    <i class="fas fa-user"></i>
    <input type="text" placeholder="User-Name" name="username">
  </div>
  <div class="textbox">
    <i class="fas fa-lock"></i>
    <input type="password" placeholder="Password" name="password">
  </div>
  <button type="submit" class="btn">SIGN UP</button></form>
  <a class="btn btn-block btn-social btn-google" style="text-align: center;background-color: #dd4b39; #dd4b39" href="/auth/google" role="button">
    <i class="fab fa-google"></i>
    Sign Up with Google
  </a>
  <a href="/login" style="color: white;float: right;font-size: small">Already have an account? Login</a>
  <h6 style="margin-top: 80px; text-align: center;">Copyright Â© 2022.</h6>
    </div>
</div>


